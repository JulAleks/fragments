{
  "ipcMode": null,
  "executionRoleArn": "arn:aws:iam::233637840829:role/LabRole",
  "taskRoleArn": "arn:aws:iam::233637840829:role/LabRole",
  "containerDefinitions": [
    {
      "name": "fragments-container",
      "image": "julal/fragments:latest",
      "cpu": 256,
      "memoryReservation": 512,
      "portMappings": [
        {
          "name": "fragments-container-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "LOG_LEVEL",
          "value": "info"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "AWS_COGNITO_USER_POOL_ID",
          "value": "${{ secrets.AWS_COGNITO_USER_POOL_ID }}"
        },
        {
          "name": "AWS_COGNITO_CLIENT_ID",
          "value": "${{ secrets.AWS_COGNITO_CLIENT_ID }}"
        },
        {
          "name": "AWS_COGNITO_REGION",
          "value": "${{ secrets.AWS_COGNITO_REGION }}"
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "value": "jalekseev-fragments"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/fragments-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "fragments-task",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
